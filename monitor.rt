defn install (reg) {
    register_component (reg, Template (“@”, ϕ, instantiator))
}

defn instantiator (reg, owner, name, template_data) {      
    name_with_id ≡ gensymbol (“@”)
    return make_leaf (name_with_id, owner, ϕ, handler)
}

defn handler (eh, msg) {
    s ⇐ msg.datum.srepr ()
    i ⇐ int (s)
    while i > 0 {
        s ⇐ #strcons (“ ”, s)
        i ⇐ i - 1
    }
    print (s)
}

% rewrite cldecode {
  text [chars+] = ‛«chars»’ 
  char_string [lq cs* rq] = ‛"«cs»"’
  char_comment [lb cs* rb] = ‛\n#| «cs» |#’
  char_ulb [c] = ‛’
  char_encodedulb [c] = ‛-L’
  char_urb [c] = ‛’
  char_encodedurb [c] = ‛R-’
  char_space [c] = ‛-’
  char_tab [c] = ‛-TAB-’
  char_newline [c] = ‛-NL-’
  char_other [c] = ‛«c»’
}

% rewrite peephole {
  main [stuff+] = ‛«stuff»’
  stuff_funcallident [f] = ‛«f»’
  stuff_default [x] = ‛«x»’
  funcallIdent [lp ws1* _funcall space ws2* _quote ident ws3* args* rp] = ‛«lp»«ident» «args»«rp»’
  args_rec [lp args* rp] = ‛«lp»«args»«rp»’
  args_default [x] = ‛«x»’

  ident [lb? first rest* rb?] = ‛«lb»«first»«rest»«rb»’
  first [c] = ‛«c»’
  rest [c] = ‛«c»’
}
